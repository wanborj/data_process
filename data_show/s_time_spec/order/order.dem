set title "Execution Order of Servant"

set grid
set key box
set autoscale fix
set xtics 0,4
set ytics 0,1
set yrange [-1:21]


set multiplot layout 2,1

set ylabel "Servant ID"
set xlabel "Times"

plot 'formate_order.data1' using 1:2 w lp pt 7 t 'Order'

set ylabel "Servant ID"
set xlabel "Times"

plot 'formate_order.data2' using 1:2 w lp pt 7 t 'Order' 


unset multiplot

